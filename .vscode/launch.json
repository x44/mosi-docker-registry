{
	"version": "0.2.0",
	"configurations": [
		//////////////////////////////////////////////////////////////////////////////////////////////////////////////
		// Runs the dev mode executable 'program'
		//////////////////////////////////////////////////////////////////////////////////////////////////////////////
		{
			"name": "Launch Program",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "cmd/mosi/.",
			"output": "_dev/program",
			"cwd": "_dev",
			"args": [
				"-dev",
			]
		},
		//////////////////////////////////////////////////////////////////////////////////////////////////////////////
		// The following run the dev mode executable 'service' which operates on the dev mode executable 'program'
		// First run the "Launch Program" configuration to have a running program executable.
		// Then run one of the "Launch Service ..." configurations to apply service commands on the program executable.
		//////////////////////////////////////////////////////////////////////////////////////////////////////////////
		{
			"name": "Launch Service Start",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "cmd/mosi/.",
			"output": "_dev/service",
			"cwd": "_dev",
			"args": [
				"-dev",
				"stop",
				"uninstall",
				"install",
				"start",
			]
		},
		{
			"name": "Launch Service Stop",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "cmd/mosi/.",
			"output": "_dev/service",
			"cwd": "_dev",
			"args": [
				"-dev",
				"stop",
				"uninstall",
			]
		},
		{
			"name": "Launch Service Status",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "cmd/mosi/.",
			"output": "_dev/service",
			"cwd": "_dev",
			"args": [
				"-dev",
				"status",
			]
		},
		//////////////////////////////////////////////////////////////////////////////////////////////////////////////
		// The following run the dev mode executable 'program' and apply a program-command
		//////////////////////////////////////////////////////////////////////////////////////////////////////////////
		{
			"name": "Launch Command 'version'",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "cmd/mosi/.",
			"output": "_dev/program",
			"cwd": "_dev",
			"args": [
				"-dev",
				"version"
			]
		},
		{
			"name": "Launch Command 'test hello'",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "cmd/mosi/.",
			"output": "_dev/program",
			"cwd": "_dev",
			"args": [
				"-dev",
				"test", "hello"
			]
		},
		//////////////////////////////////////////////////////////////////////////////////////////////////////////////
		// Run tool: configure-docker-toolbox
		//////////////////////////////////////////////////////////////////////////////////////////////////////////////
		{
			"name": "Launch Configure Docker Toolbox",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "cmd/configure-docker-toolbox/.",
			"cwd": ".",
			"console": "integratedTerminal",
			"args": [
				// "-hosts", "192.168.0.3 mosi",
				// "-certs", "_dev/certs/mosi.crt",
			]
		},
		//////////////////////////////////////////////////////////////////////////////////////////////////////////////
		// Run tool: generate-server-certificate
		//////////////////////////////////////////////////////////////////////////////////////////////////////////////
		{
			"name": "Launch Generate Server Certificate",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "cmd/generate-server-certificate/.",
			"cwd": ".",
			"console": "integratedTerminal",
			"args": [
				"name", "mosi",
				"dir", "_dev/verts",
				"hosts", "mosi",
				"ips", "192.168.0.3",
			]
		},
	]
}